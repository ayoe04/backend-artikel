{
	"apps": [{
		"name": "api-article",
		"cwd": "xampp/htdocs/tugas-backend",
		"script": "./app.js",
		"node_args": "--expose-gc",
		"watch": false,
		"ignore_watch": [
			"./node_modules",
			"./.git"
		 ],
		"log_date_format": "YYYY-MM-DD HH:mm Z",
		"exec_mode": "cluster",
		"instances": "1",
		"max_memory_restart": "512M",
		"env": {
			"PORT": "5001"
		}
	 }]
}